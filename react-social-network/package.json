{
	"name": "react-social-network",
	"version": "0.7.0",
	"description": "Simple react social app",
	"main": "index.js",
	"private": true,
	"scripts": {
		"build-css": "node-sass-chokidar src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
		"start-js": "craco start && node modify.tsconfig.js",
		"start": "NODE_OPTIONS=--max_old_space_size=8096 && npm-run-all -p watch-css start-js",
		"build-js": "craco build",
		"build": "GENERATE_SOURCEMAP=false npm-run-all build-css build-js",
		"test": "NODE_ENV=test craco test --env=jsdom --modulePaths=src --moduleNameMapper='{\"^src/(.*)$\": \"<rootDir>/src/$1\", \"^models/(.*)$\": \"<rootDir>/src/models/$1\", \"^locales/(.*)$\": \"<rootDir>/src/locales/$1\", \"^config$\": \"<rootDir>/src/config/index\"}'",
		"deploy:firebase": "npm run build && firebase deploy",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"author": "Marko Stanicic",
	"license": "MIT",
